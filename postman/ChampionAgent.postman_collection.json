{
  "info": {
    "name": "Champion Agent API",
    "description": "API collection for Champion Agent application",
    "version": "1.0.0"
  },
  "item": [
    {
      "name": "Gmail Send",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"org_id\": \"your-org-id\",\n  \"to\": \"recipient@example.com\",\n  \"subject\": \"Test Email\",\n  \"body\": \"<h1>Hello World</h1>\"\n}"
        },
        "url": {
          "raw": "{{baseUrl}}/.netlify/functions/gmail-send",
          "host": [
            "{{baseUrl}}"
          ],
          "path": [
            ".netlify",
            "functions",
            "gmail-send"
          ]
        }
      }
    },
